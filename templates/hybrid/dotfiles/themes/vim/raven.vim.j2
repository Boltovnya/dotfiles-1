hi clear

if exists("syntax on")
    syntax reset
endif

let g:colors_name="raven"

"
" preferred groups
" (:h w18)
"
hi Normal          guifg={{foreground}} guibg={{background}}    gui=none
hi Comment         guifg={{fg_comment}}    gui=none

hi Constant        guifg={{yellow}}    gui=none
hi String          guifg={{yellow}}    gui=none
hi Character       guifg={{yellow}}    gui=none
hi Number          guifg={{yellow}}    gui=none
hi Boolean         guifg={{yellow}}    gui=none
hi Float           guifg={{yellow}}    gui=none

hi Identifier      guifg={{violet}}    gui=none
hi Function        guifg={{violet}}    gui=none

hi Statement       guifg={{teal}}    gui=none
hi Conditional     guifg={{teal}}    gui=none
hi Repeat          guifg={{teal}}    gui=none
hi Label           guifg={{teal}}    gui=none
hi Operator        guifg={{teal}}    gui=none
hi Keyword         guifg={{teal}}    gui=none
hi Exception       guifg={{teal}}    gui=none

hi PreProc         guifg={{azure}}    gui=none
hi Include         guifg={{azure}}    gui=none
hi Define          guifg={{azure}}    gui=none
hi Macro           guifg={{azure}}    gui=none
hi PreCondit       guifg={{azure}}    gui=none

hi Type            guifg={{orange}}    gui=none
hi StorageClass    guifg={{orange}}    gui=none
hi Structure       guifg={{orange}}    gui=none
hi Typedef         guifg={{orange}}    gui=none

hi Special         guifg={{pink}}    gui=none
hi SpecialChar     guifg={{pink}}    gui=none
hi Tag             guifg={{pink}}    gui=none
hi Delimiter       guifg={{pink}}    gui=none
hi SpecialComment  guifg={{pink}}    gui=none
hi Debug           guifg={{pink}}    gui=none

hi Underlined      guifg=fg    gui=none
hi Ignore          guifg=bg    gui=none
hi Error           guifg=bg  guibg={{pink}}    gui=none
hi Todo            guifg={{green}}  guibg=bg        gui=none

